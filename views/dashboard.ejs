<!DOCTYPE html>
<html>
<head>
  <title><%= username %>'s Projects</title>
</head>
<body>
  <div>
    <h1>Welcome, <%= username %>!</h1>
    <div>
      <% projects.forEach(project => { %>
        <div>
          <h3><%= project.name %></h3>
          <p><%= project.description %></p>
        </div>
      <% }) %>
    </div>
  </div>
</body>
</html>
